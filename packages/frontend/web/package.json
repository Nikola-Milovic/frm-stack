    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx"
    ]
  },
  "exports": {
    "./components/*": "./src/components/*.tsx",
    "./lib/*": "./src/lib/*.ts",
    "./styles": "./src/index.css",
    "./hooks/*": "./src/hooks/*.ts"
  },
  "sideEffects": false,
  "files": [
    "src"
  ],
  "scripts": {
